/* 咨询按钮样式 */
.btn_consultation {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #a2cfa4; /* 按钮背景色 */
  border-radius: 50%; /* 完全圆形 */
  width: 40px; /* 设置按钮的宽度 */
  height: 40px; /* 设置按钮的高度，与宽度相等，形成圆形 */
  border: none;
  color: #ffffff; /* 文本颜色 */
  font-size: 16px;
  position: absolute; /* 使用绝对定位 */
  left: 10px; /* 距离左侧 10px */
  bottom: 10px; /* 距离底部 10px */
}

.btn_consultation image {
  width: 18px; /* 图标大小 */
  height: 18px; /* 图标大小 */
}
/* 去下单按钮样式 */
.btn_order {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #d1baa0; /* 按钮背景色 */
  border-radius: 8px; /* 按钮圆角 */
  padding: 12px 10px;
  border: none;
  color: #ffffff;
  font-size: 18px;
  cursor: pointer;
  position: absolute; /* 使用绝对定位 */
  right: 10px; /* 距离右侧 10px */
  bottom: 10px; /* 距离底部 10px */
}

.btn_order:active {
  background-color: #e0b481; /* 按钮按下时背景色变化 */
}
