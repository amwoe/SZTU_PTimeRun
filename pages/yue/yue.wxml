<view class="flex-col justify-start page">
  <view class="section" style="height: 1172rpx; display: block; box-sizing: border-box">
  <image style="position: absolute; left: 61rpx; top: 50rpx"
    class="image_6 pos_8"
    src="/pages/images/16/4.png"
  />
  <text class="font_3 text_5 pos_10" style="position: absolute; left: 207rpx; top: 72rpx">Deindy</text>
  <view class="flex-col section_3 pos_11">
    <view class="flex-row justify-between items-start group_2">
      <view class="flex-col items-start" style="position: relative; left: 8rpx; top: -186rpx">
        <text class="font_4 text_6">余额(元)</text>
        <text class="mt-10 text_7">{{balance}}</text>
      </view>
      <view class="flex-row items-center section_4" style="position: relative; left: -29rpx; top: -209rpx">
        
        <text class="ml-10 font_4 text_8"bindtap="navigateToDetail" style="position: relative; left: -9rpx; top: 0rpx">提现</text>
      </view>
    </view>
    <view class="flex-row justify-center equal-division section_5" style="position: relative; left: -12rpx; top: -181rpx">
      <view class="flex-col items-center group_3 equal-division-item">
        <text class="font_5 text_10">可提现</text>
        <text class="font_6 mt-15">{{balance}}</text>
      </view>
      <view class="flex-col items-center equal-division-item">
        <text class="font_5 text_9">未结算</text>
        <text class="mt-18 font_6 text_12">{{ongoing_salary}}</text>
      </view>
      <view class="flex-col equal-division-item_2 group_4">
        <text class="self-start font_5 text_11">今日收益</text>
        <text class="self-center font_6 mt-15" style="position: relative; left: -24rpx; top: 10rpx">{{completed_salary}}</text>
      </view>
    </view>
  </view>
  <view class="flex-col section_6 pos_12">

    <view class="flex-col self-stretch mt-17">
      <view
        class="flex-row items-center list-item"
        wx:for="{{items}}"
        wx:for-item="item"
        wx:for-index="index"
        wx:key="index"
      >
        <view class="flex-row items-center flex-1">
          <view class="ml-18 flex-col items-start flex-1">
            <text class="font_3 text_14">提现-00021582522</text>
            <text class="font_8 mt-13">余额：{{item.balance}}</text>
          </view>
        </view>
        <view class="flex-col shrink-0 group_5 ml-23">
          <text class="self-center font_7">{{item.amount}}</text>
          <text class="mt-16 self-start font_9 text_15">{{item.date}}</text>
        </view>
      </view>
    </view>
  </view>
</view>
