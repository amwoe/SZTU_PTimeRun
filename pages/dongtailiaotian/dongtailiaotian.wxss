.fixed-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: white; /* 可选：设置背景颜色 */
  padding: 10px; /* 可选：添加内边距 */
  box-shadow: 0 -1px 5px rgba(0, 0, 0, 0.1); /* 可选：添加阴影效果 */
}

.image-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.text {
  word-break: break-all;
}
.chat-container {
  flex-direction: column;
  height: calc(100% - 10px); /* 减去底部输入区域的高度 */
  overflow-y: auto;
  align-items: flex-end;
}
.messages {
  flex: 1;
  overflow-y: scroll;
  padding: 10px;
}
.message {
  padding: 10px;
  border-radius: 5px;
  margin-bottom: 10px;
  max-width: 70%; /* 限制消息的最大宽度 */
}
.message-right {
  align-self: flex-end;
  background-color: #e0e0e0;
}

.message-left {
  align-self: flex-start;
  background-color: #c8e6c9;
}
.sender {
  font-weight: bold;
  margin-bottom: 5px; /* 添加下边距 */
}
.content {
  margin-top: 5px;
}
.time {
  margin-top: 5px;
  font-size: 12px;
  color: #999;
}
.user-message {
  background-color: #e0e0e0;
  margin-left: auto;
  align-self: flex-end;
  border-top-left-radius: 0;
  border-top-right-radius: 5px;
  border-bottom-left-radius: 0;
  border-top-right-radius: 5px;
}
.bot-message {
  background-color: #c8e6c9;
  align-self: flex-start;
  border-top-left-radius: 5px;
  border-top-right-radius: 0;
  border-bottom-left-radius: 5px;
  border-top-right-radius: 0;
}
.input-area {
  display: flex; /* 使用flex布局 */
  align-items: center; /* 垂直居中对齐 */
  padding: 3px; /* 根据需要添加内边距 */
  background-color: #e7e4e4ee; /* 背景颜色 */
}
input {
  flex-grow: 1; /* 输入框占据剩余空间 */
  padding: 5px; /* 内边距 */
  border-radius: 5px; /* 圆角 */
  border: 1px solid rgb(194, 191, 191); /* 边框 */
}
button {
  margin-left: 10px; /* 按钮与输入框之间的间距 */
  padding: 10px; /* 内边距 */
}
.divider {
  height: 1px;
  background-color: #ddd;
  margin: 10px 0; /* 上下间距 */
}