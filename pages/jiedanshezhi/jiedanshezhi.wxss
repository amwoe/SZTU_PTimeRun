/* 按钮的基本样式 */
.btn-confirm {
  background-color: #e9d1aa; /* 按钮背景颜色 */
  color: rgb(78, 76, 76); /* 文字颜色 */
  padding: 12px 20px; /* 内边距 */
  border-radius: 40px; /* 边框圆角 */
  font-size: 16px; /* 字体大小 */
}

/* 按钮 hover 状态的样式 */
.btn-confirm:active {
  opacity: 0.5; /* 点击时的透明度 */
}