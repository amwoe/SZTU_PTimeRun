  /* 底部容器样式 */
  .bottom-container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  position: fixed;
  bottom: 60rpx;
  left: 0;
  padding: 20rpx;
  background-color: #ffffff;
  box-sizing: border-box;
  }
  
  /* 确认发布按钮样式 */
  .publish-button {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 20rpx;
  background-color: #ffae00;
  border-radius: 44rpx;
  box-sizing: border-box;
  box-shadow: 0 4rpx 8rpx rgba(0, 0, 0, 0.1);
  font-size: 36rpx;
  color: white;
}  
/* 上传按钮样式 */
.upload-button {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 20rpx;
  background-color: #ffae00; /* 橙色背景 */
  border-radius: 44rpx; /* 圆润的边框 */
  box-sizing: border-box;
  box-shadow: 0 4rpx 8rpx rgba(0, 0, 0, 0.1); /* 添加阴影 */
  font-size: 36rpx;
  color: white; /* 白色文本 */
}

/* 已上传图片样式 */
.uploaded-image {
  width: 33%; /* 图片宽度 */
  height: 100%; /* 图片高度 */
  object-fit: cover; /* 裁剪图片以填充容器 */
}

/* 发布按钮样式 */
.publish-button {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 20rpx;
  background-color: #ffae00; /* 橙色背景 */
  border-radius: 44rpx; /* 圆润的边框 */
  box-sizing: border-box;
  box-shadow: 0 4rpx 8rpx rgba(0, 0, 0, 0.1); /* 添加阴影 */
  font-size: 36rpx;
  color: white; /* 白色文本 */
}
